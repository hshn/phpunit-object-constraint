{
    "name": "hshn/phpunit-object-constraint",
    "license": "MIT",
    "authors": [
        {
            "name": "Shota Hoshino",
            "email": "sht.hshn@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "phpunit/phpunit": "^5.7.5",
        "symfony/property-access": "^3.0.9|^2.8.15"
    },
    "autoload": {
        "psr-4": {
            "Hshn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hshn\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
